{
  "name": "mine",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build:ci": "npm run build:prod && npm run storybook:build",
    "build:dev": "npx nx run-many --target=build --all=true --configuration=development",
    "build:prod": "npx nx run-many --target=build --all=true --configuration=production",
    "lint": "npx nx run-many --target=lint",
    "storybook": "nx run mine-ui:storybook",
    "test": "npx nx run-many --target=test",
    "test:ci": "npm run shared:test:ci && npm run service:test:ci && npm run ui:test:ci",
    "service:build:dev": "npx nx run mine-service:build --configuration=development",
    "service:build:prod": "npx nx run mine-service:build --configuration=production",
    "service:lint": "npx nx run mine-service:lint",
    "service:start": "nx serve mine-service",
    "service:test": "nx run mine-service:test",
    "service:test:ci": "nx run mine-service:test --configuration=ci",
    "shared:build:dev": "npx nx run-many --target=build --projects=shared-dtos,shared-models,shared-utils-datetime",
    "shared:build:prod": "npx nx run-many --target=build --projects=shared-dtos,shared-models,shared-utils-datetime",
    "shared:lint": "npx nx run-many --target=lint --projects=shared-dtos,shared-models",
    "shared:test": "npx nx run-many --target=test --projects=shared-dtos,shared-models",
    "shared:test:ci": "npx nx run shared-utils-datetime:test --configuration=ci",
    "ui:build:dev": "npx nx run mine-ui:build:development",
    "ui:build:prod": "npx nx run mine-ui:build:production",
    "ui:lint": "npx nx run mine-ui:lint",
    "ui:start": "npx nx run mine-ui:serve",
    "ui:test": "npx nx run mine-ui:test",
    "ui:test:ci": "npx nx run mine-ui:test --configuration=ci"
  },
  "private": true,
